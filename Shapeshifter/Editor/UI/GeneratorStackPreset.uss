.generator-item {
    flex-direction: row;
    flex-grow: 1;
    align-content: center;
    -unity-text-align: middle-left;
}

.generator-item .generator-item__label {
    flex-grow: 1;
    margin: 0;
    padding-left: 2px;
}

.generator-item__type {
    font-size: 10px;
    opacity: 0.5;
    padding-left: 5px;
}

.unity-list-view {
    flex-grow: 1;
}