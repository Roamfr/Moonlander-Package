}

.moonlander-analyze-content__analyze-button {
    margin-right: 0;
    flex-grow: 1;
    flex-basis: 0;
    border-top-right-radius: 0;
    border-bottom-right-radius: 0;
}

.moonlander-analyze-content__analyze-types-field > .unity-base-field__input {
