.data-registries-popup {
    background-color: #191919;
    border-width: 1px;
    border-color: #242424;
    padding: 5px 0px 5px 0px;
}

.data-registries-popup__container {
    flex-grow: 1;
}

.data-registries-popup__item .unity-text-element {
    flex-grow: 1;
    padding-left: 3px;
    -unity-text-align: middle-left;
}

.data-registries-popup__empty {
    white-space: normal;
    padding-left: 5px;
    padding-right: 5px;
    -unity-text-align: middle-center;
}