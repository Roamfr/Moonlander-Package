.moonlander-layer-stack__layer-toggle {
    position: absolute;
    top: 4px;
    left: 4px;
}

.moonlander-layer-stack__layer {
    flex-direction: row;
    flex-grow: 1;
    align-content: center;
    align-items: center;
}

.moonlander-layer-stack__layer-icon {
    width: 16px;
    height: 16px;
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_Layer.png?fileID=2800000&guid=ce56eb05a561cda498a3b12bc8522d7d&type=3#d_Layer');
}

.moonlander-layer-stack__layer > #GroupLayer {
    background-image: resource('icons/processed/d_folder icon.asset');
}

.moonlander-layer-stack__layer #DataRegistryLayer {
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_DataRegistryLayer.png?fileID=2800000&guid=e8328e53a1e619c44815bf063cc59d9e&type=3#d_DataRegistryLayer');
}

.moonlander-layer-stack__layer-label {
    flex-grow: 1;
}

.moonlander-layer-stack__layer-blend-menu {
    max-width: 100px;
}

.unity-tree-view__item-toggle {
    margin-left: 20px;
}

.moonlander-layer-stack__foldout-header .moonlander-header-foldout__header-container {
    flex-direction: row;
}

.moonlander-layer-stack__foldout-header .moonlander-header-foldout__toggle {
    margin-left: -12px;
}

.moonlander-layer-stack__header-container {
    flex-direction: row;
    height: 26px;
    padding-top: 3px;
    padding-bottom: 3px;
    background-color: var(--unity-colors-toolbar-background);
    border-bottom-width: 1px;
    border-color: rgb(50, 50, 50);
}

.moonlander-layer-stack__add-group .moonlander-layer-stack__add-button-icon {
    background-image: resource('icons/processed/d_folder icon.asset');
    width: 16px;
    height: 16px;
}

.moonlander-layer-stack__add-registry .moonlander-layer-stack__add-button-icon {
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_DataRegistryLayer.png?fileID=2800000&guid=e8328e53a1e619c44815bf063cc59d9e&type=3#d_DataRegistryLayer');
    width: 16px;
    height: 16px;
}

.moonlander-layer-stack__add-other .moonlander-layer-stack__add-button-icon {
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_Layer.png?fileID=2800000&guid=ce56eb05a561cda498a3b12bc8522d7d&type=3#d_Layer');
    width: 16px;
    height: 16px;
}

.moonlander-layer-stack__delete-button .moonlander-layer-stack__add-button-icon {
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_Delete.png?fileID=2800000&guid=d1efb7964b0e76740bbf6b261ab72495&type=3#d_Delete');
    width: 16px;
    height: 16px;
}

.moonlander-layer-stack__spacer {
    flex-grow: 1;
}

.moonlander-layer-stack__load-button .moonlander-layer-stack__add-button-icon {
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_LoadPreset.png?fileID=2800000&guid=d4d7c6ce556d7a74dbe9f4b4468707bb&type=3#d_LoadPreset');
    width: 16px;
    height: 16px;
}

.moonlander-layer-stack__save-button .moonlander-layer-stack__add-button-icon {
    background-image: resource('icons/d_preset.context.png');
    width: 16px;
    height: 16px;
}

.moonlander-layer-stack__add-button {
    align-items: center;
    justify-content: center;
    border-width: 0;
    margin-left: 1.5px;
    margin-right: 1.5px;
    margin-top: 0;
    margin-bottom: 0;
    min-width: 32px;
    min-height: 20px;
    padding-left: 3px;
    padding-right: 3px;
    background-color: rgba(0, 0, 0, 0);
    border-radius: 2px;
    flex-direction: row;
}

.moonlander-arrow-icon {
    background-image: resource('icons/d_icon dropdown.png');
    width: 12px;
    height: 12px;
}

.moonlander-layer-stack__add-button:hover {
    background-color: rgba(88, 88, 88, 0.75);
}

.moonlander-layer-stack__layers {
    flex-grow: 0;
    flex-shrink: 1;
    flex-basis: auto;
    min-height: auto;
}

.moonlander-layer-stack__layers-container {
    min-height: 120px;
}

.moonlander-layer-stack__layers-space {
    flex-grow: 1;
}

.moonlander-layer-stack {
    flex-grow: 1;
}

.moonlander-layer-stack__split {
    flex-grow: 1;
}

.moonlander-layer-stack__tabs {
    background-color: var(--unity-colors-toolbar-background);
    border-bottom-width: 1px;
    border-color: rgb(50, 50, 50);
    margin: 0;
}

.moonlander-layer-stack__tab-content {
    flex-grow: 1;
}

.unity-two-pane-split-view__dragline-anchor {
    background-color: var(--unity-colors-toolbar-border);
}

.moonlander-layer-stack__add-preset .moonlander-layer-stack__add-button-icon {
    min-width: 16px;
    min-height: 16px;
    background-image: url('project://database/Packages/com.mnlndr.moonlander/Shapeshifter/Editor/UI/Icons/d_PresetLayer.png?fileID=2800000&guid=994eb4eaa48366e4d852084352869b3e&type=3#d_PresetLayer');
}

.moonlander-layer-stack__layer-visibility {
    flex-direction: row;
    padding-top: 1px;
    padding-bottom: 1px;
    background-color: var(--unity-colors-toolbar-background);
    border-bottom-width: 1px;
    border-color: rgb(50, 50, 50);
}

.moonlander-layer-stack__opacity {
    flex-grow: 1;
    max-width: 170px;
}

.moonlander-layer-stack__blend-mode {
    flex-grow: 1;
}

.moonlander-layer-stack__opacity .unity-slider__label {
    width: 50px;
    min-width: auto;
}

.moonlander-layer-stack__layers-empty-prompt {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    padding-left: 10px;
    padding-right: 10px;
    opacity: 0.75;
    white-space: normal;
    -unity-text-align: middle-center;
    font-size: var(--unity-metrics-default-font_big_size);
}

.moonlander-layer-stack__layer-info {
    min-height: 150px;
}

.moonlander-layer-stack__tab-content--filters {
    background-color: rgb(42, 42, 42);
}

.moonlander-layer-stack__select-layer-prompt {
    position: absolute;
    left: 0;
    right: 0;
    top: 0;
    bottom: 0;
    opacity: 0.75;
    -unity-text-align: middle-center;
    font-size: var(--unity-metrics-default-font_big_size);
}

.moonlander-layer-stack__layer-info--selection .moonlander-layer-stack__select-layer-prompt {
    display: none;
}


